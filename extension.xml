<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.heyzap.sdk.extensions.googleplayservices</id>
    <versionNumber>1.0</versionNumber>

    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libGooglePlayServices.jar</nativeLibrary>
                <initializer>com.heyzap.sdk.extensions.googleplayservices.GooglePlayServicesLib</initializer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment />
        </platform>
    </platforms>
</extension>
